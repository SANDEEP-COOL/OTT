/*getting all the sideBarIcons...*/
let sideBarIcons = document.querySelectorAll(".sideBarIcon");

console.log(sideBarIcons);

/*iterating over every icon and checking that which icon is hovered...*/
sideBarIcons.forEach(icon => {

	/*if this icon is hovered then do this...*/
	icon.addEventListener('mouseover', ()=>{
		
		/*just show the sidebar here...*/
		console.log(document.getElementById("sideBar"));
		
	});
	
});